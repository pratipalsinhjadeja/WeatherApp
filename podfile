# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'WeatherForcast' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for WeatherForcast

  pod 'Alamofire', '~> 4.7'
  pod 'SwiftMessages'
  pod 'AlamofireImage', '~> 3.4'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  
  target 'WeatherForcastTests' do
    inherit! :search_paths
    # Pods for testing
    
    pod 'Alamofire', '~> 4.7'
    pod 'AlamofireImage', '~> 3.4'
    pod 'SwiftMessages'
    pod 'GoogleMaps'
    pod 'GooglePlaces'
  end

  target 'WeatherForcastUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Alamofire', '~> 4.7'
    pod 'AlamofireImage', '~> 3.4'
    pod 'SwiftMessages'
    pod 'GoogleMaps'
    pod 'GooglePlaces'
  end

end
